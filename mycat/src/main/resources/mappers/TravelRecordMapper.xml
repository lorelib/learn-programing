<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lorelib.mycat.mapper.TravelRecordMapper">
  <select id="getAll" resultType="TravelRecord">
    SELECT
    id,
    user_id,
    traveldate,
    fee,
    days
